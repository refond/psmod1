# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           com.escalesoft.psmod1.controllers.Application.index

# Map static resources from the /public folder to the /assets URL path
# Works fine with psmod1.routes and routes defined although might break while actually used in `master` application.
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

# Fails with: type Assets is not a member of package com.escalesoft.psmod1.controllers
#GET     /assets/*file               com.escalesoft.psmod1.controllers.Assets.versioned(path="/public", file: Asset)

# Fails with: type Assets is not a member of package controllers.psmod1
#GET     /assets/*file               controllers.psmod1.Assets.versioned(path="/public", file: Asset)
